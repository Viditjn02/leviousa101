{
    "name": "leviousa",
    "productName": "Leviousa",
    "version": "1.0.0",
    "description": "Leviousa - Commercial AI Meeting Assistant with Pre-configured API Keys",
    "main": "src/index.js",
    "scripts": {
        "setup": "npm install && cd leviousa_web && npm install && npm run build && cd .. && npm start",
        "start": "npm run build:renderer && electron .",
        "dev": "npm run start:with-web",
        "start:with-web": "npm run build:renderer && npm run start:web-and-electron",
        "start:web-and-electron": "node scripts/start-with-web.js",
        "package": "npm run build:all && electron-builder --dir",
        "make": "npm run build:renderer && electron-forge make",
        "build": "npm run build:all && electron-builder --config electron-builder.yml --publish never",
        "build:win": "npm run build:all && electron-builder --win --x64 --publish never",
        "publish": "npm run build:all && electron-builder --config electron-builder.yml --publish always",
        "lint": "eslint --ext .ts,.tsx,.js .",
        "postinstall": "electron-builder install-app-deps",
        "build:renderer": "node build.js",
        "build:web": "cd leviousa_web && npm run build && cd ..",
        "build:all": "npm run build:renderer && npm run build:web",
        "watch:renderer": "node build.js --watch",
        "build:dev-dmg": "LEVIOUSA_DEV_BUILD=true npm run build:all && electron-builder --config electron-builder.yml --publish never"
    },
    "keywords": [
        "leviousa",
        "ai assistant",
        "real-time",
        "transcription",
        "meeting",
        "speech-to-text",
        "ai",
        "electron",
        "desktop",
        "commercial"
    ],
    "author": {
        "name": "Leviousa Team"
    },
    "license": "UNLICENSED",
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=10.0.0"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.56.0",
        "@deepgram/sdk": "^4.9.1",
        "@google/genai": "^1.8.0",
        "@google/generative-ai": "^0.24.1",
        "@lit/react": "^1.0.8",
        "@modelcontextprotocol/sdk": "^1.17.1",
        "@useparagon/connect": "^2.0.8",
        "axios": "^1.10.0",
        "better-sqlite3": "^12.2.0",
        "cors": "^2.8.5",
        "dompurify": "^3.2.6",
        "electron-squirrel-startup": "^1.0.1",
        "electron-store": "^10.1.0",
        "electron-updater": "^6.6.2",
        "eventsource": "^3.0.7",
        "express": "^5.1.0",
        "firebase": "^12.0.0",
        "firebase-admin": "^13.4.0",
        "jsonwebtoken": "^9.0.2",
        "keytar": "^7.9.0",
        "lit-element": "^4.2.1",
        "lru-cache": "^10.4.3",
        "marked": "^15.0.12",
        "openai": "^5.10.1",
        "p-queue": "^8.1.0",
        "portkey-ai": "^1.10.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hot-toast": "^2.5.2",
        "sharp": "^0.34.2",
        "temp": "^0.9.4",
        "validator": "^13.11.0",
        "wait-on": "^8.0.3",
        "winston": "^3.17.0",
        "ws": "^8.18.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@electron/fuses": "^1.8.0",
        "@electron/notarize": "^2.5.0",
        "@typescript-eslint/eslint-plugin": "^8.39.1",
        "@typescript-eslint/parser": "^8.39.1",
        "dotenv": "^17.2.1",
        "electron": "^30.5.1",
        "electron-builder": "^26.0.12",
        "electron-reloader": "^1.2.3",
        "esbuild": "^0.25.5",
        "eslint": "^9.33.0",
        "jsdom": "^26.1.0",
        "node-fetch": "^2.6.7",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2"
    },
    "optionalDependencies": {
        "electron-liquid-glass": "^1.0.1"
    }
}
